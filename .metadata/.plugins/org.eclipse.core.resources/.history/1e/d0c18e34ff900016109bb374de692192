import java.util.ArrayList;
import java.util.Iterator;


public class ArrayListTest {

	public static void main(String[] args) {
		ArrayList<String> al=new ArrayList<String>();
		al.add("Ajeet");
		al.add("jyoti");
		al.add("deepu");
		al.add("meenakshi");
		
		//iteration using while loop
		Iterator <String> itr = al.iterator();
		while(itr.hasNext())
		{
			String s = itr.next();
			System.out.println("The array list is:" +s );
		}
		//iteration using for loop
				//for(String s:al)
				//{	
				
			  //  System.out.println("The array list is:" +s );
			  // }
		
	//to calculate the size of array list	
	int result=	al.size();
	System.out.println("the size of element is :" +result);
	//retrieving the index of element "deepu"
	int value = al.indexOf("deepu");
	System.out.println("the index of deepu element is : " +value);

	
	
}
}
