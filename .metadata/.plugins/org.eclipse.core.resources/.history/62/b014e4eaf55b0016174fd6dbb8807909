	void displayPanelHeadingNames()
	{


		System.out.println(panelHeadingName);
		System.out.println("-----------------------------------");

		for(int count =0;count<coloumList.length;count++)
		{
			if(col3 == null){
				System.out.print(coloumList[count]);
				System.out.print("\t\t\t\t\t" + col2[count]);
			}else{
				System.out.println(coloumList[count] + "\t\t\t\t\t" + col2[count]+ "\t\t\t\t\t" + col3[count]);
			}
			System.out.println("");

		}
		System.out.println("");




		/*for(int count =0;count<col2.length;count++)
		{
			System.out.print(col2[count]+ "                      " );

		}

		System.out.println("");

		for(int count =0;count<col3.length;count++)
		{
			System.out.print(col3[count]+ "                      " );

		}

		System.out.println("");

		/*for(int count=0;count<secondcolum.length;count++)
		{
			System.out.println(secondcolum[count]);
		}
		System.out.print("");
		for(int count=0;count<thirdcolum.length;count++)
		{
			System.out.println(thirdcolum[count]);
		}
	}*/



	}
}

