
public class ThreeNetWebsite {

	public static void main(String[] args) {
		
		//ThreeNetWebsite threeNetmain = new ThreeNetWebsite();
		System.out.println("===================3net processing Started====================== ");
		TPGNetMainHeader threeNetmainHeader = new TPGNetMainHeader();
