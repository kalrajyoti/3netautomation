
public class ThreeNetMainNavigationPanelDetails {

	ThreeNetMainNavigationPanelNames myTaskheading;
	ThreeNetMainNavigationPanelNames DiscussionHeading;
	ThreeNetMainNavigationPanelNames NewsLettersheading;
	

	
	       
	       
	       ThreeNetMainNavigationPanelDetails()
	       
	       {
	    	   String[] myTaskColoumName = {"Type", "Details"};
	    	   String[] firstHeadercoloum1Detail = {"Approve Attendance","No Pending request"," "};
	    	   String[] firstheadersecondrcolumsDetail = {" Approve Leaves","  no pending request", "       "};
	    
	    	 myTaskheading = new ThreeNetMainNavigationPanelNames("My Task",myTaskColoumName,firstHeadercoloum1Detail,firstheadersecondrcolumsDetail);
	    	 
	    	 String[] DiscussionColoumName = {"Title", "          Hits|Replies","Author Name"};
	    	 String[] secondheadersecondcolumsDetail = {"Selling my six month old Hyundai Xcent","68 | 0","                         Rajesh Kumar (Aug 03, 05:12 PM)"}; 
	    	 String[] secondheaderThirdcolumnDetail ={"Oops! No Holiday on Rakshabandhan..     ","70|8","                           Jyoti kalra (Aug 09, 7:09 PM"};
	    	 DiscussionHeading = new ThreeNetMainNavigationPanelNames("Discussion",DiscussionColoumName,secondheadersecondcolumsDetail,secondheaderThirdcolumnDetail);
	    	  String[] newsLetterColoumName = {"3Pedia", "Competency Center","Labs"};
	    	 String[] thirdHeadercoloum1Detail = {"3Pedia, March 2015      ","          Issue 3, November 2013" ,"Issue 2 , November 2013"};
